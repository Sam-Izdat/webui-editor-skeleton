{
  "name": "Webui Editor Skeleton",
  "scope": "https://sam-izdat.github.io/webui-editor-skeleton/",
  "short_name": "WES",
  "start_url": "https://sam-izdat.github.io/webui-editor-skeleton/",
  "display": "standalone",
  "file_handlers": [
    {
      "action": "https://sam-izdat.github.io/webui-editor-skeleton/open-file",
      "accept": {
        "application/x-my-custom-script": [
          ".myscript"
        ]
      }
    }
  ],
  "url_handlers": [
    {
      "url_pattern": "myscript://*"
    }
  ],
  "orientation": "landscape",
  "background_color": "#ffffff",
  "theme_color": "#73420d",
  "icons": [
    {
      "src": "./icons/icon-128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "description": "A web UI template for code editors."
}