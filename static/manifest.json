{
  "name": "webui-editor-skeleton",
  "scope": "https://sam-izdat.github.io/webui-editor-skeleton/",
  "short_name": "WES",
  "start_url": "https://sam-izdat.github.io/webui-editor-skeleton/index.html",
  "display": "standalone",
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/x-my-custom-script": [".myscript"]
      }
    }
  ],
  "background_color": "#ffffff",
  "theme_color": "#73420d",
  "icons": [
    {
      "src": "./icons/icon-128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}